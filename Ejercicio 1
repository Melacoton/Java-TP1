public class clasePrimaria{
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Numeros del 1 al 100 Â¿par o impar?");
    	for (int i=0;i<101;i++){
    		if(i % 2 == 0){
    		System.out.println("\nEl numero " + i + " es par\n");
    		}
    		else{
    			System.out.println(+ i);
    		}
    	}

	}

}







