package TP1;

public class clasePrimaria {
    public static void main(String[] args) {
		// TODO Auto-generated method stub
    	System.out.println("par o impar");
    	for (int i=0;i<101;i++) {
    		if(i%2==0) {
    		System.out.println("el numero es par");
    		}
    		else {
    			System.out.println(i);

    		}
    	}

	}

}






